<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qraffa.easyrentboot.dao.TestTableDao">
    <!-- 目的：为dao接口方法提供sql语句配置 -->
    <select id="queryAll" resultType="com.qraffa.easyrentboot.model.entity.TestTable">
		SELECT
			*
		FROM
			test_table
		ORDER BY
			id
	</select>
</mapper>
